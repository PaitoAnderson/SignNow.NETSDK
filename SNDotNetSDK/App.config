<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="clientID" value="" />
      <add key="clientSecret" value="" />
      <add key="apibase" value="signnow.eval" />
      <!--Copy the file to this directory for upload if the directory is not present then, create it-->
      <add key="inputdirPath" value="C:\temp\Input" />
      <!--Documents downloaded using api will be available at this location-->
      <add key="outputdirPath" value="C:\temp\Output" />
      <!--Keys added for log4net-->
      <add key="log4net.Config" value="log4.config"/>
      <add key="log4net.Config.Watch" value="True"/>
      <add key="log4net.Internal.Debug" value="False"/>
    </appSettings>
</configuration>
